{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["DOM", "ES2019"],
    "module": "ES2020",
    "moduleResolution": "node",
    "baseUrl": "./",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "~core/*": ["./pyck/core/frontend/*"],
      "~geo/*": ["./pyck/geo/frontenwd/*"]
    }
  }
}
